{
  "name": "duckgenerator",
  "displayName": "Duck Generator",
  "description": "Generates duck folder and files for the re-ducks pattern.",
  "version": "1.0.0",
  "publisher": "vanister",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vanister/duck-generator"
  },
  "icon": "images/duck-icon.png",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.generateDuck"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateDuck",
        "title": "Generate Duck"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "vscode:package": "vsce package",
    "vscode:publish": "vsce publish",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^7.0.43",
    "jest": "^23.0.1",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "vscode": "^1.1.18"
  }
}
